{
  "version": "0.1",
  "prefixes": {
    "schema": "http://schema.org/",
    "dct": "http://purl.org/dc/terms/",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "core": "http://mu.semte.ch/vocabularies/core/"
  },
  "resources": {
    "gebruikers": {
      "name": "gebruiker",
      "class": "foaf:Person",
      "attributes": {
        "created": {
          "type": "date",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "date",
          "predicate": "dct:modified"
        },
        "name": {
          "type": "string",
          "predicate": "foaf:name"
        }
      },
      "relationships": {
        "ratings": {
          "predicate": "schema:author",
          "target": "rating",
          "cardinality": "many",
          "inverse": true
        },
        "account": {
          "predicate": "foaf:account",
          "target": "useraccount",
          "cardinality": "one"
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://mu.semte.ch/services/registration-service/users/"
    },


    "useraccounts": {
      "name": "useraccount",
      "class": "foaf:OnlineAccount",
      "attributes": {
        "created": {
          "type": "date",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "date",
          "predicate": "dct:modified"
        },
        "accountname": {
          "type": "string",
          "predicate": "foaf:accountName"
        }
      },
      "relationships": {
        "gebruiker": {
          "predicate": "foaf:account",
          "target": "gebruiker",
          "cardinality": "one",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://mu.semte.ch/services/registration-service/accounts/"
    },


    "albums": {
      "name": "album",
      "class": "schema:Album",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "releasedate": {
          "type": "date",
          "predicate": "schema:datePublished"
        },
        "cover": {
          "type": "uri",
          "predicate": "schema:image"
        },
        "genre": {
          "type": "string",
          "predicate": "schema:genre"
        },
        "averagerating": {
          "type": "number",
          "predicate": "schema:ratingValue"
        }
      },
      "relationships": {
        "artists": {
          "predicate": "schema:byArtist",
          "target": "artist",
          "cardinality": "many"
        },
        "ratings": {
          "predicate": "schema:itemReviewed",
          "target": "rating",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://data.example.com/albums/"
    },


    "artists": {
      "name": "artist",
      "class": "schema:MusicGroup",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "foaf:name"
        }
      },
      "relationships": {
        "albums": {
          "predicate": "schema:byArtist",
          "target": "album",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://data.example.com/artists/"
    },


    "ratings": {
      "name": "rating",
      "class": "schema:Review",
      "attributes": {
        "score": {
          "type": "number",
          "predicate": "schema:reviewRating"
        },
        "text": {
          "type": "string",
          "predicate": "schema:reviewBody"
        },
        "creationdate": {
          "type": "datetime",
          "predicate": "dct:created"
        }
      },
      "relationships": {
        "album": {
          "predicate": "schema:itemReviewed",
          "target": "album",
          "cardinality": "one"
        },
        "gebruiker": {
          "predicate": "schema:author",
          "target": "gebruiker",
          "cardinality": "one"
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://data.example.com/ratings/"
    }
  }
}
