{
  "version": "0.1",
  "prefixes": {
    "schema": "http://schema.org/",
    "dct": "http://purl.org/dc/terms/",
    "foaf": "http://xmlns.com/foaf/0.1/"
  },
  "resources": {
    "albums": {
      "name": "album",
      "class": "schema:Album",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "releasedate": {
          "type": "date",
          "predicate": "schema:datePublished"
        },
        "cover": {
          "type": "uri",
          "predicate": "schema:image"
        },
        "genre": {
          "type": "string",
          "predicate": "schema:genre"
        }
      },
      "relationships": {
        "artists": {
          "predicate": "schema:byArtist",
          "target": "artist",
          "cardinality": "many"
        },
        "ratings": {
          "predicate": "schema:review",
          "target": "rating",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://data.example.com/albums/"
    },
    "artists": {
      "name": "artist",
      "class": "schema:MusicGroup",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "foaf:name"
        }
      },
      "relationships": {
        "albums": {
          "predicate": "schema:byArtist",
          "target": "album",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://data.example.com/artists/"
    },
    "users": {
      "name": "user",
      "class": "foaf:Person",
      "attributes": {
        "username": {
          "type": "string",
          "predicate": "foaf:nick"
        }
      },
      "relationships": {
        "ratings": {
          "predicate": "schema:review",
          "target": "rating",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://data.example.com/users/"
    },
    "ratings": {
      "name": "rating",
      "class": "schema:Review",
      "attributes": {
        "score": {
          "type": "number",
          "predicate": "schema:reviewRating"
        },
        "text": {
          "type": "string",
          "predicate": "schema:reviewBody"
        },
        "creationdate": {
          "type": "datetime",
          "predicate": "dct:created"
        }
      },
      "relationships": {
        "album": {
          "predicate": "schema:itemReviewed",
          "target": "album",
          "cardinality": "one"
        },
        "user": {
          "predicate": "schema:author",
          "target": "user",
          "cardinality": "one"
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://data.example.com/ratings/"
    }
  }
}
